# Test Environment Configuration
NODE_ENV=test
PORT=3002

# Test Database (use separate database for tests)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=ai_api_integrator_test
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=postgres
DATABASE_SSL=false
DATABASE_POOL_SIZE=5

# Test Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=1

# Test NATS
NATS_URL=nats://localhost:4222

# Test JWT Configuration
JWT_SECRET=test-jwt-secret-key-for-testing-only
JWT_EXPIRES_IN=1h
JWT_REFRESH_SECRET=test-refresh-secret-key-for-testing-only
JWT_REFRESH_EXPIRES_IN=7d

# Test OAuth (disabled in tests)
GOOGLE_CLIENT_ID=test-google-client-id
GOOGLE_CLIENT_SECRET=test-google-client-secret
GITHUB_CLIENT_ID=test-github-client-id
GITHUB_CLIENT_SECRET=test-github-client-secret

# Test S3 (use MinIO for tests)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=testkey
AWS_SECRET_ACCESS_KEY=testsecret
S3_BUCKET_NAME=test-bucket
S3_ENDPOINT=http://localhost:9000

# Test Email (disabled in tests)
SMTP_HOST=localhost
SMTP_PORT=1025
FROM_EMAIL=test@localhost

# Test Temporal (disabled in tests)
TEMPORAL_SERVER_URL=
TEMPORAL_NAMESPACE=test
TEMPORAL_TASK_QUEUE=test-flow-execution

# Test OpenAI (mocked in tests)
OPENAI_API_KEY=test-openai-key
OPENAI_MODEL=gpt-3.5-turbo

# Test Logging
LOG_LEVEL=error
LOG_FORMAT=json

# Test Features (all enabled for testing)
FEATURE_RAG_INFERENCE=true
FEATURE_TEMPORAL_WORKFLOWS=true
FEATURE_OAUTH_PROVIDERS=false
FEATURE_MAGIC_LINKS=true
FEATURE_PARTNER_SHARING=true

# Test Security (relaxed for testing)
CORS_ORIGIN=http://localhost:3000
HELMET_ENABLED=false
CSRF_PROTECTION_ENABLED=false

# Test Performance
RATE_LIMIT_MAX_REQUESTS=10000
HTTP_TIMEOUT=5000
HTTP_RETRIES=1
